{

  "compilerOptions": {
    "target": "es5",
    "declaration": true, //打包之后是否生成声明文件
    "outDir": "./lib", //输出文件
    "esModuleInterop": true,
    "lib": ["es6"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "ts-node": {
    "esm": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules","api"
  ]
}